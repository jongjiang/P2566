{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/memberNoticeRequest.schema.json",
    "title": "memberNoticeRequest",
    "description": "memberNotice request",
    "type": "object"
    "properties":{
		"appHeader": {
			"description": "memberNotice request header",
			"type": "object",
			"properties":{
				"appSourceID":{
					"description": "api發送單位",
					"type": "integer"
				},
				"appDestID":{
					"description": "api接收單位",
					"type": "integer"
				},
				"appUID":{
					"description": "訊息UID",
					"type": "string"
				},
				"appDateTime":{
					"description": "訊息日期時間",
					"type": "string"
				}
			},
			"required":["appSourceID","appDestID","appUID","appDateTime"]
		},
		"appRepBody":{
			"description": "memberNotice request body",
			"type": "object",
			"properties":{
				"noticeData":{
					"description": "通知內容",
					"type": "string"
				}
			},
			"required":["noticeData"]
		}
    }
}