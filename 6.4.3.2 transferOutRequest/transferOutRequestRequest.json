				},	
				"terminalType":{
					"description": "端末設備型態",
					"type": "string"
				},
				"txnMemo":{
					"description": "轉帳附言欄",
					"type": "string"
				},
				"transfereeAccount":{
					"description": "轉入帳號",
					"type": "string"
				},
				"transferorAccount":{
					"description": "轉出帳號",
					"type": "string"
				}				
			},
			"required":["txnAmount","transfereeID","transferorID","terminalType","transfereeAccount","transferorAccount"]
		}
    }
}