{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/amountInfoRequest.schema.json",
    "title": "amountInfoRequest",
    "description": "amountInfo request",
    "type": "object"
    "properties":{
		"appHeader": {
			"description": "apCheckOut request header",
			"type": "object",
			"properties":{
				"appSourceID":{
					"description": "api發送單位",
					"type": "integer"
				},
				"appDestID":{
					"description": "api接收單位",
					"type": "integer"
				},
				"appUID":{
					"description": "訊息UID",
					"type": "string"
				},
				"appDateTime":{
					"description": "訊息日期時間",
					"type": "string"
				}
			},
			"required":["appSourceID","appDestID","appUID","appDateTime"]
		},
		"appRepBody":{
			"description": "amountInfo request body",
			"type": "object",
			"properties":{
				"busCode":{
					"description": "跨行業務代號",
					"type": "integer"
				}
			},
			"required":["busCode"]
		}
    }
}