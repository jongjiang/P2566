{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/apCheckInResponse.schema.json",
    "title": "apCheckInResponse",
    "description": "apCheckIn response",
    "type": "object"
    "properties":{
		"appHeader": {
			"description": "apCheckIn response header",
			"type": "object",
			"properties":{
				"appSourceID":{
					"description": "api發送單位",
					"type": "integer"
				},
				"appDestID":{
					"description": "api接收單位",
					"type": "integer"
				},
				"appUID":{
					"description": "訊息UID",
					"type": "string"
				},
				"appDateTime":{
					"description": "訊息日期時間",
					"type": "string"
				}
			},
			"required":["appSourceID","appDestID","appUID","appDateTime"]
		},
		"appRepBody":{
			"description": "apCheckIn response body",
			"type": "object",
			"properties":{
				"fiscSTAN":{
					"description": "財金跨行序號",
					"type": "string"
				},
				"fiscRC":{
					"description": "財金跨行回應代碼",
					"type": "string"
				},
				"responseCode":{
					"description": "交易回應代碼",
					"type": "string"
				},
				"responseDesc":{
					"description": "交易回應敘述",
					"type": "string"
				}
			},
			"required":["fiscSTAN","fiscRC","responseCode","responseDesc"]
		}
    }
}